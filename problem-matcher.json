{
    "problemMatcher": [
        {
            "owner": "mypy-error",
            "severity": "error",
            "pattern": [
                {
                    "regexp": "^([^:]+):(\\d+):\\s+(error:\\s+.+)$",
                    "file": 1,
                    "line": 2,
                    "message": 3
                }
            ]
        }
    ]
}
